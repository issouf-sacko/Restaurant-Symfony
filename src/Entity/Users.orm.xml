<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\Users" table="users">
    <unique-constraints>
      <unique-constraint name="nomUtilisateur" columns="nomUtilisateur"/>
    </unique-constraints>
    <id name="idUser" type="integer" column="id_user">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomutilisateur" type="string" column="nomUtilisateur" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="motdepasse" type="string" column="motDePasse" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="role" type="string" column="role" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
